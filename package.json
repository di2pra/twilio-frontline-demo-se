{
  "name": "twilio-frontline-demo-se",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "start": "node build/index.js",
    "server": "nodemon --exec ts-node-esm -r dotenv/config index.ts --ignore client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "tsc --project ./",
    "heroku-postbuild": "tsc --project ./ && cd client && npm install && npm run build && mv build/ ../build/public && cd ../ && rm -rf client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hubspot/api-client": "^6.0.1-beta4",
    "@okta/jwt-verifier": "^2.3.0",
    "@types/pg": "^8.6.5",
    "@types/tmp": "^0.2.3",
    "express": "^4.17.3",
    "express-sslify": "^1.2.0",
    "node-fetch": "^3.2.3",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "redis": "^4.1.0",
    "tmp": "^0.2.1",
    "twilio": "^3.76.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-sslify": "^1.2.2",
    "@types/node": "^17.0.23",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
